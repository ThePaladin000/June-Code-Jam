{
  "version": 2,
  "builds": [
    { "src": "api/mapsServer.js", "use": "@vercel/node" },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(nearby-parks|place-details)",
      "dest": "api/mapsServer.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
